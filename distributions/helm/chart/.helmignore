# Patterns to ignore when building packages.
# This supports shell glob matching, relative path matching, and
# negation (prefixed with !). Only one pattern per line.
.DS_Store

# Common VCS dirs
.git/
.gitignore
.bzr/
.bzrignore
.hg/
.hgignore
.svn/

# Common backup files
*.swp
*.bak
*.tmp
*.orig
*~

# Various IDEs
.project
.idea/
*.tmproj
.vscode/

# Helm Build
.helmignore
Chart.lock

# Examples Directories and files
examples/
example-es-connector.sparql
example-reconciliation-config.json

# README template use by helm-docs
./README.md.gotmpl
